{
  "name": "gmopg",
  "description": "GMO PaymentGateway API client",
  "version": "0.8.0",
  "author": "linyows",
  "license": "MIT",
  "repository": "linyows/gmopg.js",
  "homepage": "https://github.com/linyows/gmopg.js",
  "keywords": [
    "api",
    "client",
    "payment"
  ],
  "bugs": "https://github.com/linyows/gmopg.js/issues",
  "main": "./index.js",
  "types": "./lib/gmopg.d.ts",
  "files": [
    "MIT-LICENSE",
    "README.md",
    "index.js",
    "lib/"
  ],
  "directories": {
    "lib": "./lib",
    "src": "./src"
  },
  "dependencies": {
    "axios": "0.16.2",
    "deepmerge": "^1.5.2"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.2",
    "@types/node": "^8.0.32",
    "ava": "^0.22.0",
    "nsp": "^2.8.1",
    "coveralls": "^3.0.0",
    "nyc": "^11.2.1",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.3"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "ava": {
    "files": [
      "./lib/**/*.test.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "ci": "npm install && npm run lint && npm run cov && nsp check",
    "test": "npm run build && ava -v",
    "cov": "npm run build && nyc ava",
    "lint": "tslint --type-check --project tsconfig.json -c tslint.json src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  }
}
