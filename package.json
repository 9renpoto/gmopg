{
  "name": "gmopg",
  "description": "GMO PaymentGateway API Client",
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.4.2"
  },
  "keywords": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "directories": {
    "dist": "./dist",
    "test": "./test"
  },
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/linyows/gmopg.js.git"
  },
  "scripts": {
    "build": "tsc",
    "ci": "npm install && npm run test && nsp check",
    "test": "npm run lint && npm run build && ava ./dist/**/*.test.js",
    "lint": "tslint --type-check --project tsconfig.json -c tslint.json src/**/*.ts",
    "clean": "rm -rf dist",
    "preversion": "npm run clean && npm run build && npm test",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "axios": "0.16.2",
    "debug": "^3.1.0",
    "deepmerge": "^1.5.2"
  },
  "devDependencies": {
    "@types/deepmerge": "^1.3.2",
    "@types/node": "^8.0.32",
    "@types/debug": "0.0.30",
    "ava": "^0.22.0",
    "nsp": "^2.8.1",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.3"
  }
}
